@model List<Category>

<h1>Category List</h1>
<br/>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Dispay Order
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
    {
        <tr>
            <td>@obj.Name</td>
            <td>@obj.DisplayOrder</td>
        </tr>
    }
    </tbody>
</table>